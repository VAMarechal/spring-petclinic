[
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ch-dev-log-group",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        },
        {
          "hostPort": 8081,
          "protocol": "tcp",
          "containerPort": 8081
        }
      ],
      "cpu": 256,
      "environment": [
        {
          "name": "MANAGEMENT_SERVER_PORT",
          "value": "8081"
        },
        {
          "name": "SERVER_PORT",
          "value": "8080"
        }
      ],
      "memory": 512,
      "image": "",
      
      "essential": true,
      "name": "userApi"
    }
]
